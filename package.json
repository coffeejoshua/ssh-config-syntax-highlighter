{
    "name": "ssh-config-syntax-highlighter",
    "displayName": "SSH Config Syntax Highlighter",
    "author": {
        "name": "Rahman Mousavian"
    },
    "homepage": "https://github.com/mousavian/ssh-config-syntax-highlighter",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.58.0"
    },
    "categories": [
